{
  "name": "Teleinfo2mqtt",
  "version": "dev",
  "slug": "teleinfo2mqtt",
  "description": "Fully configurable Teleinfo to MQTT gateway and control panel",
  "url": "https://github.com/haade-administrator/haade-addons",
  "ingress": true,
  "ingress_stream": true,
  "panel_title": "Teleinfo",
  "panel_icon": "mdi:home-lightning-bolt",
  "startup": "system",
  "init": false,
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "uart": true,
  "udev": true,
  "map": [
    "config:rw",
    "ssl:rw",
    "backup:rw"
  ],
  "options": {
    "serial": {
      "port": "/dev/ttyUSB0"
      }
  },
  "schema": {
    "log_level": "list(trace|debug|info|notice|warning|error|fatal)?",
    "serial": {
      "port": "str"
    }
  }
}
