{
  "name": "frp",
  "version": "2021.2.14",
  "slug": "frp",
  "description": "A fast reverse proxy to help you expose a local server behind a NAT or firewall to the internet.",
  "url": "https://github.com/fatedier/frp",
  "startup": "application",
  "boot": "auto",
  "host_network": true,
  "arch": ["armhf", "armv7" "amd64", "i386"],
  "options": {
    "log_level": "info",
    "common": {
    "server_addr": null,
    "server_port": "7000"
  },
    "web": {
    "type": "http",
    "local_port": "80",
    "custom_domains": "www.examples.com"
  }
  },
  "schema": {
    "log_level": "list(trace|debug|info|notice|warning|error|fatal)",
    "common": {
    "server_addr": "str",
    "server_port": "port"
    },
    "web": {
    "type": "str",
    "local_port": "port",
    "custom_domains": "str"
    }
  }
}
