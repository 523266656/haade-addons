{
  "name": "frp",
  "version": "2021.2.18",
  "slug": "frp",
  "description": "A fast reverse proxy to help you expose a local server behind a NAT or firewall to the internet.",
  "url": "https://github.com/fatedier/frp",
  "startup": "application",
  "boot": "auto",
  "host_network": true,
  "arch": ["armhf", "amd64", "i386"],
  "options": {
    "log_level": "info",
    "server_addr": null,
    "server_port": "7000",
    "authentication_method": "token",
    "token": "123456789",
    "admin_addr": "127.0.0.1",
    "admin_port": "7400",
    "admin_user": "admin",
    "admin_pwd": "pass",
    "type": "http",
#    "local_ip": "127.0.0.1",
    "local_port": "7123",
    "custom_domains": "www.examples.com",
    "custom_domains_description": "you're personnal subdomain"
  },
  "schema": {
    "log_level": "list(trace|debug|info|notice|warning|error|fatal)",
    "server_addr": "str",
    "server_port": "str",
    "proxy_name": "str?",
    "authentication_method": "str",
    "token": "str",
    "admin_addr": "str",
    "admin_port": "port",
    "admin_user": "str",
    "admin_pwd": "str",
    "type": "str",
#    "local_ip": "str",
    "local_port": "port",
    "custom_domains": "str"
  }
}
