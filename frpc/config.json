{
  "name": "frpc",
  "version": "0.1.0",
  "slug": "frp",
  "description": "A fast reverse proxy to help you expose a local server behind a NAT or firewall to the internet.",
  "url": "https://github.com/fatedier/frp",
  "startup": "application",
  "boot": "auto",
  "image": "haade/{arch}-addon-frp",
  "host_network": true,
  "arch": ["armhf", "amd64", "i386"],
  "options": {
    "common": {
    "server_addr": null,
    "server_port": "7000"
  },
    "web": {
    "type": "http",
    "local_port": "8123",
    "custom_domains": "www.examples.com"
  }
  },
  "schema": {
    "common": {
    "server_addr": "str",
    "server_port": "str"
    },
    "web": {
    "type": "str",
    "local_port": "str",
    "custom_domains": "str"
    }
  }
}
