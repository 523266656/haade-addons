{
  "name": "frp",
  "version": "2021.2.29",
  "slug": "frp",
  "description": "A fast reverse proxy to help you expose a local server behind a NAT or firewall to the internet.",
  "url": "https://github.com/fatedier/frp",
  "startup": "application",
  "boot": "auto",
  "host_network": true,
  "arch": ["armhf", "amd64", "i386"],
  "options": {
    "log_level": "info",
    "server_addr": null,
    "server_port": "7000",
    "token_key": null,
    "type": "http",
    "local_port": "8123",
    "use_encryption": "false",
    "use_compression": "true",
    "balancing_group_key": "123",
    "custom_domains": "www.examples.com",
    "server_crt": "./server.crt",
    "server_key": "./server.key"
  },
  "schema": {
    "log_level": "list(trace|debug|info|notice|warning|error|fatal)",
    "server_addr": "str",
    "server_port": "port",
    "http_name": "str?",
    "token_key": "str",
    "type": "str",
    "local_ip": "str?",
    "local_port": "port",
    "use_encryption": "list(false|true)",
    "use_compression": "list(false|true)",
    "balancing_group": "str?",
    "balancing_group_key": "str",
    "custom_domains": "str",
    "server_crt": "str",
    "server_key": "str"
  }
}
